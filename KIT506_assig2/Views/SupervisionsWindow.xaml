<Window x:Class="RAP.Views.Supervisions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Supervisions" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="231*"/>
            <RowDefinition Height="38*"/>
        </Grid.RowDefinitions>
        <ListBox Grid.Row="0" ItemsSource="{Binding Source={StaticResource StaffSupervisionsProvider}}"/>

        <Button Grid.Row="1" Content="Close" Click="Button_Click"></Button>
    </Grid>
</Window>
